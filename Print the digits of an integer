#include <stdio.h>

int main() {
   int num = 1234, digit;
   
   printf("The digits in %d are: ", num);
   
   while (num > 0) {
      digit = num % 10; // get the rightmost digit
      printf("%d ", digit); // print the digit
      num = num / 10; // remove the rightmost digit
   }
   
   return 0;
}
