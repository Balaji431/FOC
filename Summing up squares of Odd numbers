#include <stdio.h>

int main()
{
    int N, sum = 0;

    printf("Enter the value of N: ");
    scanf("%d", &N);

    for (int i = 1; i <= N; i += 2)
    {
        sum += i * i; // add the square of the current odd number to the sum
    }

    printf("The sum of the squares of odd numbers is %d\n", sum);

    return 0;
}
